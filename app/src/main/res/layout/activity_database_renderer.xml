<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    tools:context=".debug.DatabaseRendererActivity">

    <ListView
        android:id="@+id/databaseAlgorithms"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:background="@color/gray_400"
        app:layout_constraintTop_toTopOf="parent">
    </ListView>

    <ListView
        android:id="@+id/databaseLines"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        app:layout_constraintTop_toBottomOf="@id/databaseAlgorithms">

    </ListView>

    <ListView
        android:background="@color/gray_400"
        android:id="@+id/databaseNodes"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        app:layout_constraintTop_toBottomOf="@id/databaseLines">

    </ListView>

</androidx.constraintlayout.widget.ConstraintLayout>